<%= form_tag '/top/cID', method: :post, data: { turbo: false } do %>
    <%= label_tag :uid, 'ユーザーID' %>
    <%= text_field_tag :uid %>
    <%= label_tag :pass, 'パスワード' %>
    <%= password_field_tag :pass %>
    <%= label_tag :pass, 'age' %>
    <%= text_field_tag :age %>
    <%= submit_tag '登録' %>
<% end %>